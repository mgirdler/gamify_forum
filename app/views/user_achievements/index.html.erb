<h1>Listing user_achievements</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_achievements.each do |user_achievement| %>
      <tr>
        <td><%= link_to 'Show', user_achievement %></td>
        <td><%= link_to 'Edit', edit_user_achievement_path(user_achievement) %></td>
        <td><%= link_to 'Destroy', user_achievement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User achievement', new_user_achievement_path %>
